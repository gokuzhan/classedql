{
  "name": "classedql",
  "version": "1.0.0",
  "description": "ClassedQL is a promise-based Node.js ORM for MySQL",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && tsc -p .",
    "pretty": "prettier src test --write",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "docs": "typedoc",
    "postinstall": "node postinstall.js"
  },
  "types": "lib/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gokuzhan/classedql.git"
  },
  "keywords": [
    "orm",
    "mysql",
    "database",
    "helper",
    "object",
    "relational",
    "mapper",
    "nodejs"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gokuzhan/classedql/issues"
  },
  "homepage": "https://github.com/gokuzhan/classedql#readme",
  "dependencies": {
    "@types/node": "^14.0.27",
    "bluebird": "^3.7.2",
    "cli-spinner": "^0.2.10",
    "lodash": "^4.17.20",
    "table": "^5.4.6",
    "typescript": "^3.9.7",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.159",
    "chalk": "^4.1.0",
    "mysql2": "^2.1.0",
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.18.0"
  },
  "files": [
    "lib/**/*",
    "postinstall.js"
  ]
}
